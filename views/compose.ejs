<%- include("partials/header")-%>
<h1>Compose</h1>

<form action="/compose" class ="publish" method="POST">
    <div class="form-group">
        <label for="">Title</label>
        <input type="text" cols="60" name="newTitle" placeholder="New Title" autocomplete="off" class= "form-control">
    </div>
    <div class="form-group">
        <label for="">Post</label>
        <textarea name="newPost" cols="60" rows="10" class="form-control"></textarea>
    </div>
        <button type="submit" class="btn btn-primary" name="publishButton"value=<%=composeItem%>>Publish</button>

</form> 

<%- include("partials/footer")-%>